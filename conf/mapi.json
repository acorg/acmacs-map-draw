{   "_": "-*- js-indent-level: 4 -*-",

    "init": [
        {"N": "set",
         "whocc": false
        }
    ],

    "mapi": [
    ],

    "? built-in": "----------------------------------------------------------------------",

    "/all-grey": [
        {"N": "/all-color", "color": "grey80"},
        {"N": "antigens", "select": "reference", "order": "lower"},
        {"N": "sera",     "select": "all",       "order": "lower"}
    ],

    "/all-color": [
        {"N": "antigens", "select": "test",      "fill": "{color}",     "outline": "{color}"},
        {"N": "antigens", "select": "reference", "fill": "transparent", "outline": "{color}"},
        {"N": "sera",     "select": "all",       "fill": "transparent", "outline": "{color}"}
    ],

    "/size-reset": [
        {"N": "antigens", "select": "test",      "size": 5.0, "outline_width": 1.0},
        {"N": "antigens", "select": "reference", "size": 8.0, "outline_width": 1.0},
        {"N": "sera",     "select": "all",       "size": 8.0, "outline_width": 1.0}
    ],

    "/no-labels": [
        {"N": "antigens", "select": "all", "label": {"show": false}},
        {"N": "sera",     "select": "all", "label": {"show": false}}
    ],

    "/egg": [
        {"N": "antigens", "select": {"passage": "egg"}, "shape": "egg", "aspect": 1.0, "?aspect": 0.75},
        {"N": "antigens", "select": {"passage": "reassortant"}, "shape": "egg", "aspect": 1.0, "rotation": 0.2},
        {"N": "sera", "select": {"passage": "egg"}, "shape": "uglyegg", "aspect": 1.0},
        {"N": "sera", "select": {"passage": "reassortant"}, "shape": "uglyegg", "aspect": 1.0, "rotation": 0.2}
    ],

    "/continents": [
        {"N": "antigens", "select": {"continent": "EUROPE",            "date": "{date-recent}"}, "fill": "#00FF00", "outline": "black"},
        {"N": "antigens", "select": {"continent": "CENTRAL-AMERICA",   "date": "{date-recent}"}, "fill": "#AAF9FF", "outline": "black"},
        {"N": "antigens", "select": {"continent": "MIDDLE-EAST",       "date": "{date-recent}"}, "fill": "#8000FF", "outline": "black"},
        {"N": "antigens", "select": {"continent": "NORTH-AMERICA",     "date": "{date-recent}"}, "fill": "#00008B", "outline": "black"},
        {"N": "antigens", "select": {"continent": "AFRICA",            "date": "{date-recent}"}, "fill": "#FF8000", "outline": "black"},
        {"N": "antigens", "select": {"continent": "ASIA",              "date": "{date-recent}"}, "fill": "#FF0000", "outline": "black"},
        {"N": "antigens", "select": {"continent": "RUSSIA",            "date": "{date-recent}"}, "fill": "#B03060", "outline": "black"},
        {"N": "antigens", "select": {"continent": "AUSTRALIA-OCEANIA", "date": "{date-recent}"}, "fill": "#FF69B4", "outline": "black"},
        {"N": "antigens", "select": {"continent": "SOUTH-AMERICA",     "date": "{date-recent}"}, "fill": "#40E0D0", "outline": "black"},
        {"N": "antigens", "select": {"continent": "ANTARCTICA",        "date": "{date-recent}"}, "fill": "#808080", "outline": "black"},
        {"N": "antigens", "select": {"continent": "CHINA-SOUTH",       "date": "{date-recent}"}, "fill": "#FF0000", "outline": "black"},
        {"N": "antigens", "select": {"continent": "CHINA-NORTH",       "date": "{date-recent}"}, "fill": "#6495ED", "outline": "black"},
        {"N": "antigens", "select": {"continent": "CHINA-UNKNOWN",     "date": "{date-recent}"}, "fill": "#808080", "outline": "black"},
        {"N": "antigens", "select": {"continent": "UNKNOWN",           "date": "{date-recent}"}, "fill": "#808080", "outline": "black"},
        {"N": "legend", "type": "continent_map", "size": 100}
    ],

    "? clades": "----------------------------------------------------------------------",

    "/sequenced": [
        {"N": "antigens", "select": {"sequenced": true, "date": "{date-recent}", "report": true}, "?fill": "#C0C000",  "outline": "black", "legend": {"label": "sequenced ({count})"}, "order": "raise"}
    ],

    "//clades": [
        "/size-reset",
        "/all-grey",
        "/egg",
        "/clades"
    ],

    "/clades": [
        "/clades-{virus-type/lineage}{subset}"
    ],

    "/clades-pale": [
        {"N": "/clades-{virus-type/lineage}{subset}"},
        {"N": "antigens", "select": {"sequenced": true, "date": "{date-recent}"}, "fill": ":p0.5",  "outline": ":p0.8"}
    ],

    "/clades-6m": [
        {"N": "/clades-{virus-type/lineage}{subset}", "date-recent": {"younger-than-days": 183}}
    ],

    "/clades-12m": [
        {"N": "/clades-{virus-type/lineage}{subset}", "date-recent": {"younger-than-days": 365}}
    ],

    "/clades-A(H3N2)": [
        "/sequenced",
        {"N": "antigens", "select": {"clade": "3C.3",    "date": "{date-recent}"}, "fill": "#6495ED", "outline": "black", "legend": {"label": "3C.3 ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.3B",   "date": "{date-recent}"}, "fill": "blue",    "outline": "black", "legend": {"label": "3b ({count})"},   "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.2A",   "date": "{date-recent}"}, "fill": "#FF0000", "outline": "black", "legend": {"label": "2a ({count})"},   "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.2A1",  "date": "{date-recent}"}, "fill": "#8B0000", "outline": "black", "legend": {"label": "2a1 ({count})"},  "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.2A1A", "date": "{date-recent}"}, "fill": "#400000", "outline": "black", "legend": {"label": "2a1a ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.2A2",  "date": "{date-recent}"}, "fill": "#EEB422", "outline": "black", "legend": {"label": "2a2 ({count})"},  "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.3A",   "date": "{date-recent}"}, "fill": "green",   "outline": "black", "legend": {"label": "3a ({count})"},   "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.2A1B", "date": "{date-recent}"}, "fill": "#CD5C5C", "outline": "black", "legend": {"label": "2a1b ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "3C.2A1B", "date": "{date-recent}", "amino_acid": ["131T", "135K", "137F"]}, "fill": "pink",    "outline": "black", "legend": {"label": "2a1b 131T 135K 137F ({count})"}, "order": "raise"}
    ],

    "/clades-A(H1N1)2009pdm": [
        "/sequenced",

        {"?N": "antigens", "select": {"clade": "6B", "report": true},   "fill": "#6495ED", "outline": "black", "legend": {"label": "6B ({count})"}, "order": "raise"},
        {"?N": "antigens", "select": {"clade": "6B1", "report": true},  "fill": "blue",    "outline": "black", "legend": {"label": "6B1 ({count})"}, "order": "raise"},
        {"?N": "antigens", "select": {"clade": "6B2", "report": false}, "fill": "red",     "outline": "black", "legend": {"label": "6B2 ({count})"}, "order": "raise"},

        {"N": "antigens", "select": {"amino_acid": ["155E"], "date": "{date-recent}"},         "fill": "#ffc808", "legend": {"label": "155E ({count})"},      "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"amino_acid": ["155X"], "date": "{date-recent}"},         "fill": "#742f32", "legend": {"label": "155X ({count})"},      "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"amino_acid": ["156D"], "date": "{date-recent}"},         "fill": "#a2b324", "legend": {"label": "156D ({count})"},      "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"amino_acid": ["156S"], "date": "{date-recent}"},         "fill": "#049457", "legend": {"label": "156S ({count})"},      "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"amino_acid": ["156K"], "date": "{date-recent}"},         "fill": "#e72f27", "legend": {"label": "156K ({count})"},      "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"amino_acid": ["156X"], "date": "{date-recent}"},         "fill": "#f1b066", "legend": {"label": "156X ({count})"},      "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"amino_acid": ["156N" ,"155G"], "date": "{date-recent}"}, "fill": "#03569b", "legend": {"label": "156N 155G ({count})"}, "outline": "black", "order": "raise"}
    ],

    "/clades-B/Vic": [
        "/sequenced",
        {"N": "antigens", "select": {"clade": "V1",       "date": "{date-recent}", "report": true}, "fill": "blue",    "outline": "black", "legend": {"label": "1 ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "V1A",      "date": "{date-recent}", "report": true}, "fill": "#6495ED", "outline": "black", "legend": {"label": "V1.A ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "V1B",      "date": "{date-recent}", "report": true}, "fill": "red",     "outline": "black", "legend": {"label": "V1.B ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "2DEL2017", "date": "{date-recent}", "report": true}, "fill": "#DE8244", "outline": "black", "legend": {"label": "2-Del mutants ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "3DEL2017", "date": "{date-recent}", "report": true}, "fill": "#BF3EFF", "outline": "black", "legend": {"label": "3-Del mutants ({count})"}, "order": "raise"},

        {"N": "antigens", "select": {"clade": "3DEL2017", "amino_acid": ["133G"], "date": "{date-recent}"}, "fill": "#ee0000", "legend": {"label": "3-Del 133G ({count})"}, "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"clade": "3DEL2017", "amino_acid": ["133R"], "date": "{date-recent}"}, "fill": "#70ff70", "legend": {"label": "3-Del 133R ({count})"}, "outline": "black", "order": "raise"},
        {"N": "antigens", "select": {"clade": "3DEL2017", "amino_acid": ["133E"], "date": "{date-recent}"}, "fill": "#ff80ff", "legend": {"label": "3-Del 133E ({count})"}, "outline": "black", "order": "raise"}
    ],

    "/clades-B/Yam": [
        "/sequenced",
        {"N": "antigens", "select": {"clade": "Y2", "date": "{date-recent}"}, "fill": "#6495ED", "outline": "black", "legend": {"label": "Y2 ({count})"}, "order": "raise"},
        {"N": "antigens", "select": {"clade": "Y3", "date": "{date-recent}"}, "fill": "red",     "outline": "black", "legend": {"label": "Y3 ({count})"}, "order": "raise"}
    ],

    "? commands": "----------------------------------------------------------------------",

    "/pc": [
        {"N": "procrustes-arrows", "chart": 1, "projection": 0, "match": "auto", "report": true}
    ],

    "/vaccines": [
        "vaccines-{virus-type}{lineage}{subset}",
        {"N": "antigens", "select": {"vaccine": {"type": "previous"},  "passage": "cell",        "most-used-for-name": true, "report": true}, "fill": "blue", "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"},

        {"N": "antigens", "select": {"vaccine": {"type": "current"},   "passage": "egg",         "most-used-for-name": true, "report": true}, "fill": "red",  "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"},
        {"N": "antigens", "select": {"vaccine": {"type": "current"},   "passage": "cell",        "most-used-for-name": true, "report": true}, "fill": "red",  "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"},
        {"N": "antigens", "select": {"vaccine": {"type": "current"},   "passage": "reassortant", "most-used-for-name": true, "report": true}, "fill": "red",  "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"},

        {"N": "antigens", "select": {"vaccine": {"type": "surrogate"}, "passage": "egg",         "most-used-for-name": true, "report": true}, "fill": "pink", "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"},
        {"N": "antigens", "select": {"vaccine": {"type": "surrogate"}, "passage": "cell",        "most-used-for-name": true, "report": true}, "fill": "pink", "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"},
        {"N": "antigens", "select": {"vaccine": {"type": "surrogate"}, "passage": "reassortant", "most-used-for-name": true, "report": true}, "fill": "pink", "outline": "black", "size": 30, "label": {"format": "{abbreviated_name} {passage}"}, "order": "raise"}
    ],

    "? end": "*********************** END ***************************"
}
